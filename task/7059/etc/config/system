
config system 'main'
	option interface '1S'
	option mod_cdr 'off'
	option log_type 'file'
	option log_file '/ramlog/log'
	option log_size '2000'
	option syslog_enable '0'
	option log_level '6'
	option hostname 'S5V'
	option timezone 'PHT-8'
	option zonename 'Asia/Manila'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server '0.pool.ntp.org'
	list server '1.pool.ntp.org'
	list server '2.pool.ntp.org'
	list server '3.pool.ntp.org'

config telnet 'telnet'
	option port '23'
	option action 'off'

