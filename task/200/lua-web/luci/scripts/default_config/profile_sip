config sip
	option index '1'
	option name 'lan_default'
	option inbound_codec_prefs '1'
	option outbound_codec_prefs '1'
	option anonymous 'on'
	option hold_music 'on'
	option rfc2833_pt '101'
	option localport '5060'
	option localinterface 'LAN'
	option dtmf 'rfc2833'
	option progress_timeout '55'
	option session_timeout '1800'
	option inbound_codec_negotiation 'generous'
	option prack 'off'
	option allow_unknown_call 'off'
	option auth_acl '0.0.0.0/0'
	option heartbeat 'off'
	option qos 'off'

config sip
	option index '2'
	option name 'wan_default'
	option inbound_codec_prefs '1'
	option outbound_codec_prefs '1'
	option anonymous 'on'
	option hold_music 'on'
	option rfc2833_pt '101'
	option localport '5080'
	option localinterface 'WAN'
	option dtmf 'rfc2833'
	option progress_timeout '55'
	option session_timeout '1800'
	option inbound_codec_negotiation 'generous'
	option prack 'off'
	option allow_unknown_call 'off'
	option auth_acl '0.0.0.0/0'
	option ext_sip_ip 'off'
	option heartbeat 'off'
	option qos 'off'