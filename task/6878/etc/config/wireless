
config wifi-device 'ra0'
	option type 'mt7620a'
	option channel 'auto'
	option hwmode '11bgn'
	option htmode 'HT20/HT40'
	option isolate '0'
	option txpower '100'
	option wdsmode 'disable'
	option wps 'off'
	option disabled '0'

config wifi-iface 'wifi0'
	option device 'ra0'
	option ifname 'ra0'
	option index '1'
	option network 'lan'
	option mode 'ap'
	option wmm '0'
	option isolate '0'
	option disabled '0'
	option encryption 'psk2'
	option within_isolate '0'
	option ssid 'HomeULTERA_0e1627'
	option key '12345678'

